{
  "manifestVersion": "2.2",
  "contracts": {
    "IXOToken": "IXOToken",
    "ERC20Migrator": "ERC20Migrator"
  },
  "dependencies": {
    "@openzeppelin/contracts-ethereum-package": "^2.5.0"
  },
  "name": "ixo-erc20",
  "version": "1.0.0",
  "compiler": {
    "compilerSettings": {
      "optimizer": {
        "enabled": false,
        "runs": "200"
      }
    },
    "typechain": {
      "enabled": false
    },
    "manager": "truffle",
    "artifactsDir": "build/contracts",
    "contractsDir": "contracts"
  },
  "telemetryOptIn": true
}
